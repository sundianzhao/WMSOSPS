<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <!--Log4net Begin by Tony  2008.11.20-->
  <log4net>
    <appender name="ADONetAppender" type="log4net.Appender.ADONetAppender">
      <bufferSize value="1" />
      <connectionType value="System.Data.SqlClient.SqlConnection, System.Data, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      <connectionString value="DATABASE=RTDBOSPS;SERVER=.;UID=sa;PWD=123;Connect Timeout=30;" />
      <commandText value="INSERT INTO Sys_LogDetail ([CreateTime],[Thread],[LogLevel],[Logger],[Message],[Exception],[UserName],[UserIP],[CurrentUrl],[PrevUrl],[OperationType],[ToObjct],[ModelType],[ViewLevel],[FromMethod],[IPCity]) VALUES (@log_date, @thread, @log_level, @logger, @Message, @exception,@UserName,@UserIP,@CurrentUrl,@PrevUrl,@OperationType,@ToObjct,@ModelType,@ViewLevel,@FromMethod,@IPCity)" />
      <parameter>
        <parameterName value="@log_date" />
        <dbType value="DateTime" />
        <layout type="log4net.Layout.RawTimeStampLayout" />
      </parameter>
      <parameter>
        <parameterName value="@thread" />
        <dbType value="String" />
        <size value="100" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%t" />
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@log_level" />
        <dbType value="String" />
        <size value="200" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%p" />
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@logger" />
        <dbType value="String" />
        <size value="500" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%logger" />
        </layout>
      </parameter>
      <!--<parameter>
        <parameterName value="@message" />
        <dbType value="String" />
        <size value="3000" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%message" />
        </layout>
      </parameter>-->
      <parameter>
        <parameterName value="@exception" />
        <dbType value="String" />
        <size value="40000" />
        <layout type="log4net.Layout.ExceptionLayout" />
      </parameter>
      <!--<parameter>
        <parameterName value="@location" />
        <dbType value="String" />
        <size value="2000" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%location" />
        </layout>

      </parameter>-->
      <!--自定义UserName -->
      <parameter>
        <parameterName value="@Message" />
        <dbType value="String" />
        <size value="5000" />
        <layout type="Exam.Cloud.Code.Logger.MyLayout" >
          <conversionPattern value = "%property{Message}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@UserName" />
        <dbType value="String" />
        <size value="50" />
        <layout type="Exam.Cloud.Code.Logger.MyLayout" >
          <conversionPattern value = "%property{UserName}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@UserIP" />
        <dbType value="String" />
        <size value="50" />
        <layout type="Exam.Cloud.Code.Logger.MyLayout" >
          <conversionPattern value = "%property{UserIP}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@CurrentUrl" />
        <dbType value="String" />
        <size value="500" />
        <layout type="Exam.Cloud.Code.Logger.MyLayout" >
          <conversionPattern value = "%property{CurrentUrl}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@PrevUrl" />
        <dbType value="String" />
        <size value="500" />
        <layout type="Exam.Cloud.Code.Logger.MyLayout" >
          <conversionPattern value = "%property{PrevUrl}"/>
        </layout>
      </parameter>

      <parameter>
        <parameterName value="@OperationType" />
        <dbType value="String" />
        <size value="100" />
        <layout type="Exam.Cloud.Code.Logger.MyLayout" >
          <conversionPattern value = "%property{OperationType}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@ToObjct" />
        <dbType value="String" />
        <size value="500" />
        <layout type="Exam.Cloud.Code.Logger.MyLayout" >
          <conversionPattern value = "%property{ToObjct}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@ModelType" />
        <dbType value="String" />
        <size value="100" />
        <layout type="Exam.Cloud.Code.Logger.MyLayout" >
          <conversionPattern value = "%property{ModelType}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@ViewLevel" />
        <dbType value="String" />
        <size value="100" />
        <layout type="Exam.Cloud.Code.Logger.MyLayout" >
          <conversionPattern value = "%property{ViewLevel}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@FromMethod" />
        <dbType value="String" />
        <size value="500" />
        <layout type="Exam.Cloud.Code.Logger.MyLayout" >
          <conversionPattern value = "%property{FromMethod}"/>
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@IPCity" />
        <dbType value="String" />
        <size value="500" />
        <layout type="Exam.Cloud.Code.Logger.MyLayout" >
          <conversionPattern value = "%property{IPCity}"/>
        </layout>
      </parameter>
    </appender>
    <logger name="MyLog">
      <level value="DEBUG" />
      <!--根据log级别记录到不同的日志文件-->
      <appender-ref ref="InfoLog" />
      <appender-ref ref="DebugLog" />
      <appender-ref ref="ErrorLog" />
      <appender-ref ref="WarnLog" />
      <appender-ref ref="ADONetAppender"/>
    </logger>

    <appender name="InfoLog" type="log4net.Appender.RollingFileAppender">
      <!--INFO、DEBUG级别的日志文件路径，根据自己的需要修改-->
      <file value="Logs\Info\" />
      <!--是否覆盖，默认是追加true-->
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <!--文件名称-->
      <DatePattern value="yyyy-MM-dd'.txt'"></DatePattern>
      <!--设置无限备份=-1 ，最大备份数为1000-->
      <param name="MaxSizeRollBackups" value="1000" />
      <!--每个文件的大小-->
      <param name="MaximumFileSize" value="10240KB" />
      <!--最小锁定模型以允许多个进程可以写入同一个文件-->
      <param name="lockingModel"  type="log4net.Appender.FileAppender+MinimalLock" />
      <!--名称是否可以更改为false为可以更改-->
      <param name="StaticLogFileName" value="false" />
      <!--使用Unicode编码-->
      <Encoding value="UTF-8" />
      <layout type="log4net.Layout.PatternLayout">
        <!--输出格式-->
        <!--%n【出错的类】%logger 属性[%property{NDC}]-->
        <conversionPattern value= "%n【记录时间】%date %message" />
      </layout>
      <securityContext type="log4net.Util.WindowsSecurityContext">
        <credentials value="Process" />
      </securityContext>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="DEBUG" />
        <levelMax value="INFO" />
      </filter>
    </appender>
    <appender name="DebugLog" type="log4net.Appender.RollingFileAppender">
      <!--WARN级别的日志文件路径，根据自己的需要修改-->
      <file value="Logs\Debug\" />
      <!--是否覆盖，默认是追加true-->
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <!--文件名称-->
      <DatePattern value="yyyy-MM-dd'.txt'"></DatePattern>
      <!--设置无限备份=-1 ，最大备份数为1000-->
      <param name="MaxSizeRollBackups" value="1000" />
      <!--每个文件的大小-->
      <param name="MaximumFileSize" value="10240KB" />
      <!--最小锁定模型以允许多个进程可以写入同一个文件-->
      <param name="lockingModel"  type="log4net.Appender.FileAppender+MinimalLock" />
      <!--名称是否可以更改为false为可以更改-->
      <param name="StaticLogFileName" value="false" />
      <!--使用Unicode编码-->
      <Encoding value="UTF-8" />
      <layout type="log4net.Layout.PatternLayout">
        <!--输出格式-->
        <!--%n【出错的类】%logger 属性[%property{NDC}]-->
        <conversionPattern value=
                           "%n＝＝＝＝＝＝＝＝＝＝
                            %n【日志级别】%-5level
                            %n【记录时间】%date
                            %n【线程编号】[%thread]
                            %n【执行时间】[%r]毫秒
                            %n【出错文件】%F
                            %n【出错行号】%L
                            %n%message" />
      </layout>
      <securityContext type="log4net.Util.WindowsSecurityContext">
        <credentials value="Process" />
      </securityContext>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="WARN" />
        <levelMax value="WARN" />
      </filter>
    </appender>
    <appender name="ErrorLog" type="log4net.Appender.RollingFileAppender">
      <!--ERROR级别的日志文件路径，根据自己的需要修改-->
      <file value="Logs\Error\" />
      <!--是否覆盖，默认是追加true-->
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <!--文件名称-->
      <DatePattern value="yyyy-MM-dd'.txt'"></DatePattern>
      <!--设置无限备份=-1 ，最大备份数为1000-->
      <param name="MaxSizeRollBackups" value="1000" />
      <!--每个文件的大小-->
      <param name="MaximumFileSize" value="10240KB" />
      <!--最小锁定模型以允许多个进程可以写入同一个文件-->
      <param name="lockingModel"  type="log4net.Appender.FileAppender+MinimalLock" />
      <!--名称是否可以更改为false为可以更改-->
      <param name="StaticLogFileName" value="false" />
      <!--使用Unicode编码-->
      <Encoding value="UTF-8" />
      <layout type="log4net.Layout.PatternLayout">
        <!--输出格式-->
        <conversionPattern value=
                           "%n＝＝＝＝＝＝＝＝＝＝
                            %n【日志级别】%-5level
                            %n【记录时间】%date
                            %n【线程编号】[%thread]
                            %n【执行时间】[%r]毫秒
                            %n【出错文件】%F
                            %n【出错行号】%L
                            %n%message" />
      </layout>
      <securityContext type="log4net.Util.WindowsSecurityContext">
        <credentials value="Process" />
      </securityContext>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR" />
        <levelMax value="ERROR" />
      </filter>
    </appender>
    <appender name="WarnLog" type="log4net.Appender.RollingFileAppender">
      <!--ERROR级别的日志文件路径，根据自己的需要修改-->
      <file value="Logs\Warn\" />
      <!--是否覆盖，默认是追加true-->
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <!--文件名称-->
      <DatePattern value="yyyy-MM-dd'.txt'"></DatePattern>
      <!--设置无限备份=-1 ，最大备份数为1000-->
      <param name="MaxSizeRollBackups" value="1000" />
      <!--每个文件的大小-->
      <param name="MaximumFileSize" value="10240KB" />
      <!--最小锁定模型以允许多个进程可以写入同一个文件-->
      <param name="lockingModel"  type="log4net.Appender.FileAppender+MinimalLock" />
      <!--名称是否可以更改为false为可以更改-->
      <param name="StaticLogFileName" value="false" />
      <!--使用Unicode编码-->
      <Encoding value="UTF-8" />
      <layout type="log4net.Layout.PatternLayout">
        <!--输出格式-->
        <conversionPattern value=
                           "%n＝＝＝＝＝＝＝＝＝＝
                            %n【日志级别】%-5level
                            %n【记录时间】%date
                            %n【线程编号】[%thread]
                            %n【执行时间】[%r]毫秒
                            %n【出错文件】%F
                            %n【出错行号】%L
                            %n%message" />
      </layout>
      <securityContext type="log4net.Util.WindowsSecurityContext">
        <credentials value="Process" />
      </securityContext>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="WARN" />
        <levelMax value="WARN" />
      </filter>
    </appender>

  </log4net>
  <!--Log4net End-->
</configuration>